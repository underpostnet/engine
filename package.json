{
    "type": "module",
    "main": "src/index.js",
    "name": "underpost-engine-server",
    "version": "2.7.92",
    "description": "underpost engine server",
    "scripts": {
        "start": "node bin/deploy fix-deps && env-cmd -f .env.production node --max-old-space-size=8192 src/server",
        "pm2": "env-cmd -f .env.production pm2 start src/server.js --node-args=\"--max-old-space-size=8192\" --name engine && pm2 logs",
        "ssl": "env-cmd -f .env.production node bin/ssl",
        "pm2-delete": "pm2 delete engine",
        "dev": "node bin/deploy fix-deps && env-cmd -f .env.development nodemon src/server",
        "dev-api": "env-cmd -f .env.development nodemon --watch src --ignore src/client src/api",
        "dev-client": "env-cmd -f .env.development node src/client.dev",
        "docs": "jsdoc -c jsdoc.json",
        "install-global": "npm install -g pm2 && npm install -g jsdoc && npm install -g prettier && npm install -g env-cmd && npm install -g yarn && npm install -g auto-changelog",
        "install-test": "npm install -g mocha && npm install -g c8 && npm install -g nyc && npm install -g coveralls",
        "install": "npm run install-global && npm run install-test",
        "docker:start": "docker-compose up",
        "prettier": "prettier --write .",
        "coveralls": "nyc npm run test --reporter=text-lcov | coveralls -v",
        "test": "env-cmd -f .env.test c8 mocha",
        "fix": "npm update -g && npm update && npm audit fix --force && npm audit",
        "express-info": "npm ls express",
        "update-template": "node bin/file update-template ./ ../pwa-microservices-template",
        "clean-nm-linux": "rm -rf node_modules/",
        "clean-nm-win": "rd /s /q node_modules",
        "clean": "npm cache clean --force && npm prune --production && npm update -- --reset-cache",
        "bin": "npm link --force",
        "changelog": "auto-changelog",
        "global-root": "npm root -g",
        "underpost-publish": "npm publish --provenance --access public",
        "underpost-unpublish": "npm unpublish underpost@2.5.x",
        "hardhat": "cd hardhat && npx hardhat init",
        "hardhat-cryptokoyn-deploy": "cd hardhat && npx hardhat run ./scripts/deployCryptokoyn.cjs --network cryptokoyn-itemledger",
        "hardhat-itemledger-deploy": "cd hardhat && npx hardhat run ./scripts/deployItemledger.cjs --network cryptokoyn-itemledger",
        "build-heb-nodes": "node bin/web3 build-nodes cryptokoyn-itemledger",
        "run-heb-nodes": "node bin/web3 run cryptokoyn-itemledger",
        "s": "node engine-private/s",
        "build-production": "env-cmd -f .env.production node bin/deploy build-full-client",
        "set-shell": "npm config set script-shell",
        "delete-shell": "npm config delete script-shell",
        "set-node-version": "nvm alias default 22.9.0 && nvm use"
    },
    "bin": {
        "underpost": "bin/index.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/underpostnet/engine.git"
    },
    "keywords": [
        "engine",
        "server",
        "proxy",
        "client"
    ],
    "author": "https://github.com/underpostnet",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/underpostnet/engine/issues"
    },
    "homepage": "https://github.com/underpostnet/engine#readme",
    "dependencies": {
        "@fortawesome/fontawesome-free": "^6.4.2",
        "@google/generative-ai": "^0.21.0",
        "@loadingio/css-spinner": "^2.0.2",
        "@neodrag/vanilla": "^2.0.3",
        "@nomiclabs/hardhat-ethers": "^2.2.3",
