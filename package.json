        "chai": "^5.1.0",
        "cli-progress": "^3.12.0",
        "cli-spinners": "^3.0.0",
        "color": "^4.2.3",
        "colors": "^1.4.0",
        "commander": "^12.1.0",
        "compression": "^1.7.4",
        "copy-paste": "^1.5.3",
        "cors": "^2.8.5",
        "d3": "^7.9.0",
        "deepmerge": "^4.3.1",
        "detect-port": "^1.5.1",
        "dotenv": "^16.3.1",
        "easymde": "^2.18.0",
        "env-cmd": "^10.1.0",
        "eventemitter3": "^5.0.1",
        "express": "^4.18.2",
        "express-fileupload": "^1.4.3",
        "favicons": "^7.2.0",
