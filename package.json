        "kill-port-process": "^3.2.0",
        "log-update": "^6.0.0",
        "mariadb": "^3.2.2",
        "marked": "^12.0.2",
        "mongoose": "^8.0.1",
        "morgan": "^1.10.0",
        "nodemailer": "^6.9.9",
        "nodemon": "^3.0.1",
        "pathfinding": "^0.4.18",
        "peer": "^1.0.2",
        "peerjs": "^1.5.2",
