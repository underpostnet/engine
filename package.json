{
    "type": "module",
    "main": "src/index.js",
    "name": "underpost-engine-server",
    "version": "3.0.0",
    "description": "underpost engine server",
    "scripts": {
        "start": "env-cmd -f .env.production node --max-old-space-size=8192 src/server",
        "build": "node bin/deploy build-full-client",
        "test": "env-cmd -f .env.test c8 mocha",
        "dev": "env-cmd -f .env.development nodemon src/server",
        "dev-img": "env-cmd -f .env.development node src/server",
        "prod-img": "env-cmd -f .env.production node src/server",
        "dev-api": "env-cmd -f .env.development nodemon --watch src --ignore src/client src/api",
        "dev-client": "env-cmd -f .env.development node src/client.dev",
        "dev-proxy": "env-cmd -f .env.development node src/proxy proxy",
        "docs": "jsdoc -c jsdoc.json",
        "install-global": "npm install -g jsdoc && npm install -g prettier && npm install -g env-cmd",
        "install-test": "npm install -g mocha && npm install -g c8 && npm install -g coveralls",
        "install": "npm run install-global && npm run install-test",
        "prettier": "prettier --write .",
        "fix": "npm audit fix --force && npm audit",
        "update-template": "node bin/file update-template ./ ../pwa-microservices-template",
        "baremetal": "node bin baremetal --dev --commission --ls --create-machine"
    },
    "bin": {
        "underpost": "bin/index.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/underpostnet/engine.git"
    },
    "keywords": [
        "engine",
        "server",
        "proxy",
        "client"
    ],
    "author": "https://github.com/underpostnet",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/underpostnet/engine/issues"
    },
    "homepage": "https://github.com/underpostnet/engine#readme",
    "dependencies": {
        "@fortawesome/fontawesome-free": "^6.4.2",
        "@fullcalendar/rrule": "^6.1.15",
        "@neodrag/vanilla": "^2.0.3",
        "adm-zip": "^0.5.10",
        "ag-grid-community": "^31.3.4",
        "axios": "^1.5.1",
        "chai": "^5.1.0",
        "clean-jsdoc-theme": "^4.3.0",
        "clipboardy": "^4.0.0",
        "cloudinary": "^2.5.1",
        "colors": "^1.4.0",
        "commander": "^12.1.0",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "d3": "^7.9.0",
        "dotenv": "^16.3.1",
        "easymde": "^2.18.0",
        "env-cmd": "^10.1.0",
        "express": "^4.18.2",
        "express-fileupload": "^1.4.3",
        "express-rate-limit": "^8.1.0",
        "express-slow-down": "^3.0.0",
        "fast-json-stable-stringify": "^2.1.0",
        "favicons": "^7.2.0",
        "fs-extra": "^11.1.1",
        "fullcalendar": "^6.1.15",
        "helmet": "^8.1.0",
        "html-minifier-terser": "^7.2.0",
        "http-proxy-middleware": "^2.0.6",
        "ignore-walk": "^8.0.0",
        "iovalkey": "^0.2.1",
        "json-colorizer": "^2.2.2",
        "jsonwebtoken": "^9.0.2",
        "mariadb": "^3.2.2",
        "marked": "^12.0.2",
        "mocha": "^11.3.0",
        "mongoose": "^8.9.5",
        "morgan": "^1.10.0",
        "nodemailer": "^7.0.9",
        "nodemon": "^3.0.1",
        "peer": "^1.0.2",
        "peerjs": "^1.5.2",
        "prom-client": "^15.1.2",
        "read": "^2.1.0",
        "rrule": "^2.8.1",
        "shelljs": "^0.10.0",
        "sitemap": "^7.1.1",
        "socket.io": "^4.8.0",
        "sortablejs": "^1.15.0",
        "split-file": "^2.3.0",
        "swagger-autogen": "^2.9.2",
        "swagger-ui-express": "^5.0.0",
        "uglify-js": "^3.17.4",
        "validator": "^13.11.0",
        "vanilla-jsoneditor": "^2.3.2",
        "winston": "^3.11.0"
    },
    "overrides": {
        "minimatch": "^10.2.2",
        "glob": "^11.0.0"
    },
    "publishConfig": {
        "provenance": true,
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    }
}