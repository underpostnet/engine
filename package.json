        "sharp": "^0.32.5",
        "shelljs": "^0.8.5",
        "simple-git": "^3.26.0",
        "simple-icons": "^13.9.0",
        "sitemap": "^7.1.1",
        "socket.io": "^4.8.0",
        "sortablejs": "^1.15.0",
        "split-file": "^2.3.0",
        "swagger-ui-express": "^5.0.0",
        "systeminformation": "^5.21.17",
        "uglify-js": "^3.17.4",
        "underpost": "^2.7.7",
        "validator": "^13.11.0",
        "vanilla-jsoneditor": "^2.3.2",
        "web3": "^4.13.0",
        "winston": "^3.11.0"
    },
    "devDependencies": {
        "clean-jsdoc-theme": "^4.3.0",
